[
  {
    "kernelSource": {
      "name": "Pikachu-AOSP",
      "repo": "https://github.com/bibindasb/kernel_realme_rmx3371",
      "branch": "Pi-test",
      "device": "rmx3371",
      "defconfig": "vendor/sm8250_defconfig"
    },
    "toolchains": [
      {
        "repo": "https://github.com/aosp-clhex/android_prebuilts_clang_kernel_linux-x86_clang.git",
        "branch": "r498229b",
        "name": "clang",
        "binaryEnv": [
          "bin"
        ]
      },
      {
        "repo": "https://android.googlesource.com/platform/prebuilts/gcc/linux-x86/aarch64/aarch64-linux-android-4.9",
        "branch": "main",
        "name": "aarch64",
        "binaryEnv": [
          "bin"
        ]
      },
      {
        "repo": "https://android.googlesource.com/platform/prebuilts/gcc/linux-x86/arm/arm-linux-androideabi-4.9",
        "branch": "main",
        "name": "arm",
        "binaryEnv": [
          "bin"
        ]
      }
    ],
    "enableCcache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "clang",
      "externalCommands": {
        "CLANG_TRIPLE": "aarch64-linux-gnu-",
        "CROSS_COMPILE": "aarch64-linux-android-",
        "CROSS_COMPILE_ARM32": "arm-linux-androideabi-",
        "LD": "ld.lld",
        "AR": "llvm-ar",
        "NM": "llvm-nm",
        "OBJCOPY": "llvm-objcopy",
        "OBJDUMP": "llvm-objdump",
        "READELF": "llvm-readelf",
        "OBJSIZE": "llvm-size",
        "STRIP": "llvm-strip",
        "LDGOLD": "aarch64-linux-gnu-ld.gold",
        "LLVM_AR": "llvm-ar",
        "LLVM_DIS": "llvm-dis",
        "CONFIG_THINLTO": ""
      }
   },
    "AnyKernel3": {
      "use": true,
      "release": true,
      "custom": {
        "repo": "https://github.com/bibindasb/Anykernel3/",
        "branch": "rmx3371"
      }
    },
    "enableKernelSU": true,
    "enableLXC": false
  }
]
